{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RateLimiting": {
    "PermitLimit": 60,
    "WindowSeconds": 60
  },
  "PowerShellService": {
    "MaxTasks": 100,
    "TaskExpiryMinutes": 1440,
    "TaskTimeoutMinutes": 720
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:22333"
      }
    }
  }
}
