{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RateLimiting": {
    "PermitLimit": 240,
    "WindowSeconds": 60
  },
  "PowerShellService": {
    "MaxTasks": 100,
    "CompletedTaskRetentionMinutes": 10080,
    "TaskTimeoutMinutes": 1440
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:22333"
      }
    }
  }
}
